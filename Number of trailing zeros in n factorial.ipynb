    "def trailing_zero(n):\n",
    "    factorial=1\n",
    "    for i in range(1,n+1):\n",
    "        factorial=factorial*i\n",
    "    count=0\n",
    "    while factorial%10==0:\n",
    "        factorial=factorial//10\n",
    "        count=count+1\n",
    "    return count\n",
    "print(trailing_zero(100))"
   ]
