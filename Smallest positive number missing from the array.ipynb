    "class Solution(object):\n",
    "    def minimumpositive(self,nums):\n",
    "        n=set(nums)\n",
    "        i=1\n",
    "        while i in n:\n",
    "            i+=1\n",
    "        return i\n",
    "\n",
    "s=Solution()\n",
    "arr=[2,1,7,6,-1,-5]\n",
    "print(s.minimumpositive(arr))"
   ]
